{
  "name": "crosschain-widget",
  "description": "Crosschain Governance Widget",
  "version": "1.1.0",
  "type": "module",
  "license": "AGPL-3.0-only",
  "main": "./dist/CrossChainWidget.umd.js",
  "module": "./dist/CrossChainWidget.es.js",
  "types": "./dist/index.d.ts",
  "repository": "git@github.com:defi-wonderland/crosschain-widget-private.git",
  "author": "Wonderland",
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:dev": "vite build -c ./vite.config.dev.ts --outDir build",
    "preview": "vite preview --outDir build",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "yarn run lint -- --fix",
    "prettier": "prettier src --write",
    "format": "yarn run prettier:fix && npm run lint:fix",
    "format:check": "yarn run prettier && npm run lint",
    "prepare": "husky install && wonderland-crypto-husky-checks install",
    "test": "vitest run --dir src/test/",
    "test:watch": "vitest  --dir src/test/"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "src/**/*.{js,jsx,ts,tsx,css,scss,md,html}": "prettier --write --ignore-unknown"
  },
  "peerDependencies": {
    "react": "16.8.0 || >=17.x",
    "react-dom": "16.8.0 || >=17.x"
  },
  "dependencies": {
    "@connext/nxtp-txservice": "2.0.0-alpha.1",
    "@connext/nxtp-utils": "2.0.1",
    "@sinclair/typebox": "0.25.21",
    "ethers": "5.6.8",
    "ethers-multicall": "0.2.3",
    "memoizee": "0.4.15",
    "react-transition-group": "4.4.5",
    "styled-components": "5.3.9"
  },
  "devDependencies": {
    "@defi-wonderland/crypto-husky-checks": "1.3.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "14.0.0",
    "@testing-library/user-event": "14.4.3",
    "@types/memoizee": "0.4.8",
    "@types/node": "18.15.11",
    "@types/react": "18.0.37",
    "@types/react-dom": "18.0.11",
    "@types/react-transition-group": "4.4.5",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.59.0",
    "@typescript-eslint/parser": "5.59.0",
    "@vitejs/plugin-react": "3.1.0",
    "eslint": "8.38.0",
    "eslint-config-prettier": "8.8.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react": "7.32.2",
    "eslint-plugin-react-hooks": "4.6.0",
    "husky": "8.0.3",
    "jsdom": "21.1.1",
    "lint-staged": "13.2.1",
    "prettier": "2.8.7",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.0.4",
    "vite": "4.3.9",
    "vite-plugin-dts": "2.3.0",
    "vite-tsconfig-paths": "4.2.0",
    "vitest": "0.30.1"
  }
}
